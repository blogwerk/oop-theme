<!DOCTYPE HTML>
<html {{ fn.language_attributes() }}>
<head>
    <title>{% block title %}{% endblock %}</title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="description" content="{{ fn.get_bloginfo('description') }}" />

    <!--[if lte IE 8]>{{ fn.wp_print_scripts('html5shiv') }}<![endif]-->

    {{ fn.wp_head() | raw }}
</head>
<body {{ fn.body_class() }}>

{% block header %}
    {% embed 'layout/header.html.twig' %}{% endembed  %}
{% endblock %}

{% block content %}{% endblock %}

{% block sidebar %}{% endblock %}

{% block footer %}
    {% embed 'layout/footer.html.twig' %}{% endembed %}
{% endblock %}

{{ fn.wp_footer() | raw }}

</body>
</html>